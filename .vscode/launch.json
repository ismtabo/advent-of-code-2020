    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Deno",
            "request": "launch",
            "type": "pwa-node",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--inspect-brk",
                "--allow-all",
            ],
            "args": [
                "--part1",
                "partOne/sample.in"
            ],
            "attachSimplePort": 9229
        }
    ]
}